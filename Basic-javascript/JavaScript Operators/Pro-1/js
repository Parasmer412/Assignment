// Declare two numbers
let num1 = 20;
let num2 = 5;

// Addition
let sum = num1 + num2;
console.log("Addition: " + sum); // 25

// Subtraction
let difference = num1 - num2;
console.log("Subtraction: " + difference); // 15

// Multiplication
let product = num1 * num2;
console.log("Multiplication: " + product); // 100

// Division
let quotient = num1 / num2;
console.log("Division: " + quotient); // 4
